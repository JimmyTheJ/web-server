{
    "Logging": {
        "IncludeScopes": false,
        "LogLevel": {
            "Default": "Debug",
            "System": "Information",
            "Microsoft": "Information"
        }
    },
    "ConnectionStrings": {
        "WebServerDbConnectionString": "Server=YOUR_SERVER;Database=YOUR_DB;Trusted_Connection=true;MultipleActiveResultSets=true"
    },
    "Options": {
        "DatabaseType": 1,
        "Well-Known": false
    },
    "Jwt": {
        "Issuer": "YOUR_IP_ADDRESS_AND_PORT_OR_DOMAIN",
        "Audience": "YOUR_IP_ADDRESS_AND_PORT_OR_DOMAIN",
        "SigningKey": "YOUR_PASSWORD"
    }
}
